{
  "name": "web-lib",
  "version": "1.0.10",
  "description": "Moberries web-lib",
  "main": "lib/index.js",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib"
    ]
  },
  "scripts": {
    "lint": "standard | snazzy",
    "lint:fix": "standard --fix | snazzy",
    "start": "start-storybook -p 3001",
    "build-storybook": "build-storybook",
    "build:watch": "cross-env BABEL_ENV=production babel src -d lib -w --ignore __tests__,stories.js,container.js && git add .",
    "build:prod": "rimraf lib && cross-env BABEL_ENV=production babel src -d lib --ignore __tests__,stories.js,container.js && git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moberries/web-lib.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moberries/web-lib/issues"
  },
  "pre-commit": [
    "lint",
    "build:prod"
  ],
  "homepage": "https://github.com/moberries/web-lib#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "pre-commit": "^1.2.2",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "style-loader": "^0.17.0",
    "surge": "^0.19.0",
    "url-loader": "^0.5.8"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "create-react-class": "^15.5.3",
    "cross-env": "^5.0.0",
    "date-fns": "^1.28.4",
    "history": "^4.6.1",
    "linkifyjs": "^2.1.4",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "rc-progress": "^2.1.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.13.1",
    "react-image-gallery": "^0.8.0",
    "react-input-range": "^1.1.3",
    "react-modal": "^1.7.7",
    "react-notification": "^6.7.0",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.4",
    "react-textarea-autosize": "^4.3.2",
    "react-tooltip": "^3.3.0",
    "validator": "^7.0.0",
    "yup": "^0.21.3"
