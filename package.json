{
  "name": "web-lib",
  "version": "1.0.36",
  "description": "Moberries web-lib",
  "main": "lib/index.js",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib"
    ]
  },
  "scripts": {
    "lint": "standard | snazzy",
    "lint:fix": "standard --fix | snazzy",
    "start": "start-storybook -p 3001",
    "build-storybook": "build-storybook",
    "build:watch": "cross-env BABEL_ENV=production babel src -d lib -w --ignore __tests__,stories.js,container.js && git add .",
    "build:prod": "rimraf lib && cross-env BABEL_ENV=production babel src -d lib --ignore __tests__,stories.js,container.js && git add .",
    "deploy": "surge storybook-static styleguide.moberries.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moberries/web-lib.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moberries/web-lib/issues"
  },
  "pre-commit": [
    "lint",
    "build:prod"
  ],
  "homepage": "https://github.com/moberries/web-lib#readme",
  "devDependencies": {
    "@storybook/react": "^3.2.17",
    "autoprefixer": "^7.2.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "pre-commit": "^1.2.2",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-form": "7.1.2",
    "resolve-url-loader": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "surge": "^0.19.0",
    "url-loader": "^0.6.2"
  },
  "dependencies": {
    "browser-image-size": "^1.1.0",
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.2",
    "cross-env": "^5.1.1",
    "date-fns": "^1.29.0",
    "history": "^4.7.2",
    "linkifyjs": "^2.1.5",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "rc-progress": "^2.2.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.3",
    "react-image-gallery": "^0.8.6",
    "react-input-range": "^1.2.2",
    "react-modal": "^3.1.6",
    "react-notification": "^6.8.2",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.1.0",
    "react-textarea-autosize": "^5.2.1",
    "react-tooltip": "^3.4.0",
