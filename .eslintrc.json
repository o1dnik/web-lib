{
  "root": true,
  "extends": [
    "react-app",
    "standard",
    "standard-jsx",
    "standard-react",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/no-unused-prop-types": "off",
    "react/prop-types": "off",
    "jsx-a11y/href-no-hash": ["off"],
    "react/no-deprecated": ["off"],
    "camelcase": [
      "off"
    ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  }
}
